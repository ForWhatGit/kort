
<label for="private">Links</label>
<%if (!singleStudy.private) { %>
	<div class="radio">
		<label><input type="radio" name="private" id="private" value='false' checked>All participants use same link</label>
		(<a href="<%= url %>/study/<%= singleStudy._id %>"><%= url %>/study/<%= singleStudy._id %></a>)
	</div>
	<div class="radio">
		<label><input type="radio" name="private" id="private" value='true'>Unique Link for each participant</label>
	</div>
<% } else { %> 
	<div class="radio">
		<label><input type="radio" name="private" id="private" value='false'>All participants use same link</label>
		(<a href="<%= url %>/study/<%= singleStudy._id %>"><%= url %>/study/<%= singleStudy._id %></a>)
	</div>
	<div class="radio">
		<label><input type="radio" name="private" id="private" value='true' checked>Unique Link for each participant</label>
	</div>
<% } %>
