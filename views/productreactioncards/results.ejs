<!DOCTYPE html>
<html lang="en">
	<head>
		<%- include('../partials/header.ejs') %>
		<%- include('../partials/datatables.ejs') %>
		<script type="text/javascript" src="/plotlyjs/plotly.min.js"></script>
		<script type="text/javascript" src="/js/results.js"></script>

		<title>Desirability Results</title>
	</head>
	<body>
		<%- include('../partials/admin_header.ejs') %>

		<div class='container'>
			<h2><%= study.title %></h2>
			<hr>
			<div class="well">
				<%- include('../partials/resultstable.ejs') %>
			</div>
			<div class="well">
			<div id='bargraph'></div>
			</div>
		</div>
		<script>
		var data = [{
			type: 'bar',
			x: <%- JSON.stringify(counts) %>,
			y: <%- JSON.stringify(words) %>,
			orientation: 'h',
		}];
		var layout = {
			title: '<%=study.title %>  - Results',
			autosize: true,
			margin: {
				l: 150,
				pad: 5,
			},
		};

		Plotly.newPlot('bargraph', data, layout);

		</script>
	</body>
</html>
